{
  "name": "clientlibs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint-scripts": "eslint --ext .jsx,.js ./src/",
    "lint-styles": "stylelint \"./src/**/*.scss\"",
    "karma": "NODE_ENV=test karma start",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "pretest": "npm run lint-scripts && npm run lint-styles",
    "test": "npm run karma"
  },
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.4.0"
  },
  "dependencies": {
    "es6-promise": "4.2.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.2",
    "svgxuse": "1.2.6",
    "url-search-params-polyfill": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "aem-clientlib-generator": "^1.4.1",
    "aemsync": "^3.0.2",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "directory-named-webpack-plugin": "^4.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jquery": "^3.3.1",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-notify-reporter": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "node-sass-glob-importer": "^5.3.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "^3.5.4",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "ie 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx,json}": [
      "npm run lint-scripts --",
      "prettier-eslint --write",
      "git add"
    ],
    "src/*.{css,scss}": [
      "npm run lint-styles --",
      "prettier --write",
      "git add"
    ]
  }
}
